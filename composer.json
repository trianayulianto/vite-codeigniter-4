{
    "name": "trianayulianto/vite-codeigniter-4",
    "description": "Vite backend integration for CodeIgniter 4",
    "keywords": [
        "vite",
        "codeigniter"
    ],
    "type": "library",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "TYlnt\\Vite\\": "src/"
        }
    },
    "authors": [
        {
            "name": "Triana Yulianto",
            "email": "triana.yulianto2018@gmail.com"
        }
    ],
    "require": {
        "php": "^8.0",
        "codeigniter4/framework": "^4.0.2"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^3.13.0",
        "phpunit/phpunit": "^9.3.0"
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
